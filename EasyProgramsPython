b = int(input())
for i in range(a, b+1):
     print(i, end=' ')
print(tuple(range(a,b+1)))


____________________________________________
a = int(input())
b = int(input())
if a<=b:
    for i in range(a, b+1):
        print(i, end=' ')
else:
    for i in range(b, a+1):
        print(i, end=' ')

