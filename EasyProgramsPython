def power(a, n):
    if n == 0:
        return 1    
    return a * power(a, n - 1)
     

a = float(input()) 
n = int(input()) 
print(power(a, n))


__________________________---



a = int(input())
n = int(input())


def power(a, n):
    if n == 0:
        return 1
    return a * power(a, n - 1)


def ologin(a, n):
    if n % 2 == 0:
        return (a * a) ** (n // 2)
    else:
        return a * a ** (n - 1)


print(ologin(a, n))


_______________________________________-

n = int(input()) 
m = int(input()) 

def rf(a, b):
    while a != 0 and b != 0:
        if a > b:
            a = a % b
        else:
            b = b % a
 
    return (a + b)
      
    
nm =  rf(n, m)       
print(int(n/nm),int(m/nm))


